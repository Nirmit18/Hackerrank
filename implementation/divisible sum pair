
#include <iostream>

using namespace std;

int main()
{
   int i,count=0,t,m,k;
   cin>>m>>k;
   int a[m];
   for(i=0;i<m;i++)
   {
       cin>>a[i];
   }
   for(i=0;i<m;i++)
   {
       int j=i+1;
     while(j<m)
     {
       t=a[i]+a[j]; 
       j++;
       if(t%k==0)
       {
           count++;
       }
       
     }
     
   }
   
   cout<<count;

    return 0;
}
